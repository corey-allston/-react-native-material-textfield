{
  'parser': 'babel-eslint',

  'plugins': [
    'import',
    'react',
  ],

  'extends': [
    'eslint:recommended',
    'plugin:import/errors',
    'plugin:import/warnings',
    'plugin:react/recommended',
  ],

  'rules': {
    'comma-dangle': ['error', 'always-multiline'],
  },
}

